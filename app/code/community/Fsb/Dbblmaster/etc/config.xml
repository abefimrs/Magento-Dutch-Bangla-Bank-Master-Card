<config>
    <modules>
        <Fsb_Dbblmaster>
            <version>0.0.1</version>
        </Fsb_Dbblmaster>
    </modules>
    <global>
        <models>
            <dbblmaster>
                <class>Fsb_Dbblmaster_Model</class>
            </dbblmaster>
        </models>
        <blocks>
            <dbblmaster>
                <class>Fsb_Dbblmaster_Block</class>
            </dbblmaster>
        </blocks>
        <resources>
            <dbblmaster_setup>
                <setup>
                    <module>Fsb_Dbblmaster</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </dbblmaster_setup>
            <dbblmaster_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </dbblmaster_write>
            <dbblmaster_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </dbblmaster_read>
        </resources>
        <helpers>
            <dbblmaster>
                <class>Fsb_Dbblmaster_Helper</class>
            </dbblmaster>
        </helpers>
    </global>
    <frontend>
        <secure_url>
            <dbblmaster_redirect>/dbblmaster/redirect/</dbblmaster_redirect>
        </secure_url>
        <routers>
            <dbblmaster>
                <use>standard</use>
                <args>
                    <module>Fsb_Dbblmaster</module>
                    <frontName>dbblmaster</frontName>
                </args>
            </dbblmaster>
        </routers>
        <translate>
            <modules>
                <Fsb_Dbblmaster>
                    <files>
                        <default>Fsb_Dbblmaster.csv</default>
                    </files>
                </Fsb_Dbblmaster>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Fsb_Dbblmaster>
                    <files>
                        <default>Fsb_Dbblmaster.csv</default>
                    </files>
                </Fsb_Dbblmaster>
            </modules>
        </translate>
    </adminhtml>
    <default>
        <payment>
            <dbblmaster>
                <active>0</active>
                <model>dbblmaster/checkout</model>
                <title>Dutch Bangla Bank Master Card</title>
                <dbblmaster_wallet>0000001</dbblmaster_wallet>
                <order_status>2</order_status>
				<sort_order>2</sort_order>
            </dbblmaster>
        </payment>
    </default>
</config>
